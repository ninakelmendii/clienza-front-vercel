{"ast":null,"code":"import UserControl from '@/components/Navbar/UserControl.vue';\nimport * as LANGUAGE from '../../../store/language.json';\nimport NavbarControl from '@/components/Navbar/NavbarControl.vue';\nimport FeedbackInfo from './FeedbackInfo.vue';\nexport default {\n  name: \"FeedbackDetailsComponent\",\n  data() {\n    return {\n      title: \"Feedback Details\"\n    };\n  },\n  methods: {\n    translate(word) {\n      let wordIndex = null;\n      LANGUAGE.forEach((lng, index) => {\n        if (lng.english.toLowerCase() == word.toLowerCase()) {\n          wordIndex = index;\n        }\n      });\n      if (wordIndex === null) {\n        return word;\n      }\n      if (this.$store.state.language == 'german') {\n        return LANGUAGE[wordIndex].german;\n      } else {\n        return LANGUAGE[wordIndex].english;\n      }\n    }\n  },\n  components: {\n    NavbarControl,\n    UserControl,\n    FeedbackInfo\n  }\n};","map":{"version":3,"names":["UserControl","LANGUAGE","NavbarControl","FeedbackInfo","name","data","title","methods","translate","word","wordIndex","forEach","lng","index","english","toLowerCase","$store","state","language","german","components"],"sources":["src/components/Feedback/FeedbackDetails/FeedbackDetails.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <v-row>\r\n      <v-col cols=\"12\" md=\"12\" lg=\"12\">\r\n        <v-row>\r\n          <v-col cols=\"12\">\r\n            <v-row>\r\n              <v-col cols=\"12\" md=\"8\" class=\"d-flex justify-start\">\r\n                <NavbarControl />\r\n                <h2 style=\"display: flex; align-items: center;\">{{ translate(title) }}</h2>\r\n              </v-col>\r\n              <v-col cols=\"12\" md=\"4\" style=\"margin-right: -12px;\"><UserControl /></v-col>\r\n            </v-row>\r\n          </v-col>\r\n          <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\" lg=\"12\">\r\n            <FeedbackInfo />\r\n          </v-col>\r\n        </v-row>\r\n      </v-col>\r\n