n                        </v-date-picker>\r\n                    </v-menu>\r\n                </v-col>\r\n                <v-col cols=\"12\" md=\"6\">\r\n                    <v-text-field\r\n                        :label=\"translate('Hobbys')\"\r\n                        v-model=\"info.hobby\"\r\n                        solo\r\n                        class=\"input-protocol\"\r\n                    ></v-text-field>\r\n                </v-col>\r\n            </v-row>\r\n            <v-row>\r\n                <v-col cols=\"12\" md=\"6\">\r\n                    <v-text-field\r\n                        :label=\"translate('Profession')\"\r\n                        v-model=\"info.profession\"\r\n                        solo\r\n                        class=\"input-protocol\"\r\n                    ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" md=\"6\">\r\n                    <v-text-field\r\n                        :label=\"translate('Number of persons in the household')\"\r\n                        v-model=\"info.nr_family_members\"\r\n                        type=\"number\"\r\n                        solo\r\n                        class=\"input-protocol\"\r\n                    ></v-text-field>\r\n                </v-col>\r\n            </v-row>\r\n            <v-row no-gutters style=\"height: 100%; margin-top: 50px;\" justify=\"end\">\r\n                <div>\r\n                    <v-btn\r\n                        color=\"#36A5DD\"\r\n                        class=\"btn-next-page\"\r\n                        @click=\"$emit('next-page', info)\"\r\n                        >{{ translate('Next') }}</v-btn>\r\n                </div>\r\n            </v-row>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport * as LANGUAGE from '../../store/language.json';\r\n\r\n    export default {\r\n        name: 'FirstStep',\r\n        props: ['defaultData'],\r\n        data(){return{\r\n            menu: false,\r\n            info: {\r\n                first_name: '',\r\n                last_name: '',\r\n                street_nr: '',\r\n                plz: '',\r\n                profession: '',\r\n                nr_family_members: null,\r\n                birthday_date: null,\r\n                hobby: null,\r\n            },\r\n        }},\r\n        computed: {\r\n            birthday() {\r\n                if (!this.info.birthday_date) return null\r\n\r\n                const [year, month, day] = this.info.birthday_date.split('-')\r\n                return `${day}.${month}.${year}`;\r\n            },\r\n            canNextPage() {\r\n                if(this.info.name != '' && this.info.vorname != '' && this.info.strasse != '' && this.info.plz != '' &&  this.info.beruf != '' && this.date != null) {\r\n                    return true;\r\n                }\r\n\r\n                return false;\r\n            }\r\n        },\r\n        methods: {\r\n            translate(word) {\r\n                let wordIndex = null;\r\n\r\n                LANGUAGE.forEach((lng, index) => {\r\n                    if(lng.english.toLowerCase() == word.toLowerCase()) {\r\n                    wordIndex = index;\r\n                    }\r\n                });\r\n\r\n                if(wordIndex === null) {\r\n                    return word;\r\n                }\r\n\r\n                if(this.$store.state.language == 'german') {\r\n                    return LANGUAGE[wordIndex].german;\r\n                } else {\r\n                    return LANGUAGE[wordIndex].english;\r\n                }\r\n            },\r\n            bindParams() {\r\n                if(this.defaultData != undefined) {\r\n                    console.log('running');\r\n                    this.info = this.defaultData;\r\n                }\r\n            }\r\n        },\r\n        created() {\r\n            this.bindParams();\r\n            setTimeout(() => {\r\n                this.bindParams();\r\n            }, 1000);\r\n        }\r\n    }\r\n</script>"],"mappings":"AAmIA,YAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;EACAC,KAAA;IAAA;MACAC,IAAA;MACAC,IAAA;QACAC,UAAA;QACAC,SAAA;QACAC,SAAA;QACAC,GAAA;QACAC,UAAA;QACAC,iBAAA;QACAC,aAAA;QACAC,KAAA;MACA;IACA;EAAA;EACAC,QAAA;IACAC,SAAA;MACA,UAAAV,IAAA,CAAAO,aAAA;MAEA,OAAAI,IAAA,EAAAC,KAAA,EAAAC,GAAA,SAAAb,IAAA,CAAAO,aAAA,CAAAO,KAAA;MACA,UAAAD,GAAA,IAAAD,KAAA,IAAAD,IAAA;IACA;IACAI,YAAA;MACA,SAAAf,IAAA,CAAAJ,IAAA,eAAAI,IAAA,CAAAgB,OAAA,eAAAhB,IAAA,CAAAiB,OAAA,eAAAjB,IAAA,CAAAI,GAAA,eAAAJ,IAAA,CAAAkB,KAAA,eAAAC,IAAA;QACA;MACA;MAEA;IACA;EACA;EACAC,OAAA;IACAC,UAAAC,IAAA;MACA,IAAAC,SAAA;MAEA5B,QAAA,CAAA6B,OAAA,EAAAC,GAAA,EAAAC,KAAA;QACA,IAAAD,GAAA,CAAAE,OAAA,CAAAC,WAAA,MAAAN,IAAA,CAAAM,WAAA;UACAL,SAAA,GAAAG,KAAA;QACA;MACA;MAEA,IAAAH,SAAA;QACA,OAAAD,IAAA;MACA;MAEA,SAAAO,MAAA,CAAAC,KAAA,CAAAC,QAAA;QACA,OAAApC,QAAA,CAAA4B,SAAA,EAAAS,MAAA;MACA;QACA,OAAArC,QAAA,CAAA4B,SAAA,EAAAI,OAAA;MACA;IACA;IACAM,WAAA;MACA,SAAAC,WAAA,IAAAC,SAAA;QACAC,OAAA,CAAAC,GAAA;QACA,KAAArC,IAAA,QAAAkC,WAAA;MACA;IACA;EACA;EACAI,QAAA;IACA,KAAAL,UAAA;IACAM,UAAA;MACA,KAAAN,UAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}