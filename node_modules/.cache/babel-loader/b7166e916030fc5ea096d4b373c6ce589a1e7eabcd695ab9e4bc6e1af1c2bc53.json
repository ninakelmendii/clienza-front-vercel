{"ast":null,"code":"import axios from \"axios\";\nimport * as LANGUAGE from '../../../store/language.json';\nexport default {\n  data: () => ({\n    dialog: false,\n    dialogDelete: false,\n    headers: [{\n      text: \"First Name\",\n      align: \"start\",\n      value: \"first_name\"\n    }, {\n      text: \"Last Name\",\n      value: \"last_name\"\n    },\n    // { text: \"Type\", value: \"type\" },\n    {\n      text: \"Date\",\n      value: \"created_at\"\n    }, {\n      text: \"Actual Insurance\",\n      value: \"investment\"\n    }, {\n      text: \"KVG\",\n      value: \"kvg.name\"\n    }, {\n      text: \"VVG\",\n      value: \"vvg.name\"\n    },\n    // { text: \"Status\", value: \"status\" },\n    {\n      text: \"Actions\",\n      value: \"actions\"\n    }],\n    people: [],\n    insurances: [],\n    editedIndex: -1,\n    editedItem: {\n      first_name: \"\",\n      last_name: \"\",\n      type: \"\",\n      date: \"\",\n      price: 0,\n      status: \"\"\n    },\n    defaultItem: {\n      first_name: \"\",\n      last_name: \"\",\n      type: \"\",\n      date: \"\",\n      price: 0,\n      status: \"\"\n    },\n    deleteItemId: 0,\n    updateItemId: 0,\n    loading: true,\n    options: {},\n    totalTableData: 0,\n    data: []\n  }),\n  computed: {\n    formTitle() {\n      return this.editedIndex === -1 ? \"New Item\" : \"Edit Item\";\n    },\n    headersComputed() {\n      const computed_headers = [{\n        text: this.translate(\"First Name\"),\n        align: \"start\",\n        value: \"first_name\"\n      }, {\n        text: this.translate(\"Last Name\"),\n        value: \"last_name\"\n      },\n      // { text: \"Type\", value: \"type\" },\n      {\n        text: this.translate(\"Date\"),\n        value: \"created_at\"\n      }, {\n        text: this.translate(\"Actual Insurance\"),\n        value: \"investment\"\n      }, {\n        text: this.translate(\"KVG\"),\n        value: \"kvg.name\"\n      }, {\n        text: this.translate(\"VVG\"),\n        value: \"vvg.name\"\n      },\n      // { text: \"Status\", value: \"status\" },\n      {\n        text: this.translate(\"Actions\"),\n        value: \"actions\"\n      }];\n      return computed_headers;\n    }\n  },\n  watch: {\n    dialog(val) {\n      val || this.close();\n    },\n    dialogDelete(val) {\n      val || this.closeDelete();\n    },\n    options: {\n      handler() {\n        this.getItemsTable();\n      },\n      deep: true\n    }\n  },\n  methods: {\n    translate(word) {\n      let wordIndex = null;\n      LANGUAGE.forEach((lng, index) => {\n        if (lng.english.toLowerCase() == word.toLowerCase()) {\n          wordIndex = index;\n        }\n      });\n      if (wordIndex === null) {\n        return word;\n      }\n      if (this.$store.state.language == 'german') {\n        return LANGUAGE[wordIndex].german;\n      } else {\n        return LANGUAGE[wordIndex].english;\n      }\n    },\n    getInsuranceName(id) {\n      const insurance = this.insurances.find(i => i.id == id);\n      return insurance.name;\n    },\n    emitEdit(people_id) {\n      this.$emit('editPeople', true, people_id);\n    },\n    getItemsTable() {\n      this.loading = true;\n      const {\n        sortBy,\n        sortDesc,\n        page,\n        itemsPerPage\n      } = this.options;\n      let sortType;\n      sortType = \"asc\";\n      if (sortDesc[0]) {\n        sortType = \"desc\";\n      }\n      let id = this.$router.currentRoute.params.id;\n      let url = 'leadpeople';\n      let param = 'lead_id';\n      if (!id) {\n        id = this.$router.currentRoute.params.client_id;\n        url = 'customerpeople';\n        param = 'customer_id';\n      }\n\n      // Getting the insurances\n      axios.get('/insurances').then(response => {\n        console.log('Insurances', response.data);\n        this.insurances = response.data.data;\n      }).catch(err => console.log(err));\n\n      // axios.get('customerpeople/datatable?customer_id=' + id).then(response => console.log('CUSTOMER PEOPLES', response.data))\n\n      axios.get(`/${url}/datatable?${param}=${id}`, {\n        params: {\n          sort_by: sortBy[0],\n          sort: sortType,\n          page: page,\n          per_page: itemsPerPage\n        }\n      }).then(response => {\n        console.log(\"aksdnlaksjhdakjsljdhkalsd\", response.data);\n        this.data = response.data.data;\n        this.totalTableData = response.data.total;\n        this.loading = false;\n        this.$nextTick(() => {\n          const currentEl = this.$el.querySelector(\".v-data-footer__pagination\");\n          const translated = currentEl.innerHTML.replace(\"of\", this.translate(\"of\"));\n          currentEl.innerHTML = translated;\n        });\n      }).catch(error => {\n        console.log(error);\n      });\n      this.loading = false;\n    },\n    deleteItem(item) {\n      let url = '/lead/people/';\n      if (this.$router.currentRoute.params.client_id) {\n        url = \"customer_people/\";\n      }\n      axios.delete(url + item).then(response => {\n        this.snackbar = {\n          message: response.data.message,\n          color: \"success\",\n          show: true\n        };\n      }).catch(error => {\n        this.snackbar = {\n          message: error.response.data.message,\n          color: \"error\",\n          show: true\n        };\n      });\n    },\n    deleteItemConfirm() {\n      this.deleteItemModalConfirm();\n    },\n    close() {\n      this.dialog = false;\n    },\n    deleteItemModal(item) {\n      this.deleteItemId = item;\n      this.dialogDelete = true;\n    },\n    closeDelete() {\n      this.dialogDelete = false;\n    },\n    deleteItemModalConfirm() {\n      this.closeDelete();\n      this.deleteItem(this.deleteItemId);\n      this.getItemsTable();\n    }\n  }\n};","map":{"version":3,"names":["axios","LANGUAGE","data","dialog","dialogDelete","headers","text","align","value","people","insurances","editedIndex","editedItem","first_name","last_name","type","date","price","status","defaultItem","deleteItemId","updateItemId","loading","options","totalTableData","computed","formTitle","headersComputed","computed_headers","translate","watch","val","close","closeDelete","handler","getItemsTable","deep","methods","word","wordIndex","forEach","lng","index","english","toLowerCase","$store","state","language","german","getInsuranceName","id","insurance","find","i","name","emitEdit","people_id","$emit","sortBy","sortDesc","page","itemsPerPage","sortType","$router","currentRoute","params","url","param","client_id","get","then","response","console","log","catch","err","sort_by","sort","per_page","total","$nextTick","currentEl","$el","querySelector","translated","innerHTML","replace","error","deleteItem","item","delete","snackbar","message","color","show","deleteItemConfirm","deleteItemModalConfirm","deleteItemModal"],"sources":["src/components/Leads/LeadDetails/PeopleTable.vue"],"sourcesContent":["<template>\r\n  <v-data-table\r\n      :headers=\"headersComputed\"\r\n      :items=\"data\"\r\n      :loading=\"loading\"\r\n      :options.sync=\"options\"\r\n      :server-items-length=\"totalTableData\"\r\n      :footer-props=\"{'items-per-page-text':translate('Rows per page'), 'items-per-page-all-text':translate('All')}\"\r\n  >\r\n     <template v-slot:loading>\r\n      {{translate('Loading items')}}...\r\n    </template>\r\n    <template v-slot:no-data>\r\n      {{translate('No data available')}}\r\n    </template>\r\n    <template v-slot:top>\r\n      <v-toolbar flat>\r\n        <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"text-h5\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                    <v-text-field\r\n                      v-model=\"editedItem.first_name\"\r\n                      :label=\"translate('First Name')\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                    <v-text-field\r\n                      v-model=\"editedItem.last_name\"\r\n                      :label=\"translate('Last Name')\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                    <v-text-field\r\n                      v-model=\"editedItem.type\"\r\n                      :label=\"translate('Type')\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                    <v-text-field\r\n                      v-model=\"editedItem.date\"\r\n                      :label=\"translate('Date')\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                    <v-text-field\r\n                      v-model=\"editedItem.price\"\r\n                      :label=\"translate('Price')\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                    <v-text-field\r\n                      v-model=\"editedItem.status\"\r\n                      :label=\"translate('Status')\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"close\"> Cancel </v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"save\"> Save </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n        <v-dialog v-model=\"dialogDelete\" max-width=\"550px\">\r\n          <v-card>\r\n          <v-row no-gutters no-wrap style=\"padding: 24px;\">\r\n              <img src=\"@/assets/gifs/trash.gif\" style=\"width: 56px; height: 56px;\" />\r\n              <div style=\"margin-left: 16px;\">\r\n                  <v-card-title class=\"text-h5 p-0\">{{translate('Delete')}}</v-card-title>\r\n                  <v-card-text class=\"p-0\" style=\"margin-top: 4px;\">{{ translate('Are you sure you want to delete this item?') }}</v-card-text>\r\n              </div>\r\n          </v-row>\r\n          <v-card-actions class=\"justify-end\">\r\n              <button @click=\"closeDelete\" class=\"btn btn-cancel\" :class=\"{ 'btn-dark': $vuetify.theme.dark }\"\r\n                  >{{ translate('Cancel') }}</button\r\n              >\r\n              <button\r\n                  color=\"blue darken-1\"\r\n                  text\r\n                  @click=\"deleteItemConfirm\"\r\n                  class=\"btn btn-decline\"\r\n                  >{{translate('Confirm')}}</button\r\n              >\r\n          </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:[`item.actions`]=\"{ item }\">\r\n      <div>\r\n        <v-btn icon @click=\"emitEdit(item.id)\">\r\n          <svg width=\"18\" height=\"18\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\r\n            <path fill-rule=\"evenodd\" clip-rule=\"evenodd\" d=\"M8.78486e-06 22.1066C0.00104677 22.6084 0.20068 23.0895 0.555672 23.4443C0.910482 23.7993 1.39158 23.9989 1.89343 24H20.2404C20.7423 23.9993 21.2234 23.7995 21.5784 23.4447C21.9334 23.0897 22.133 22.6086 22.1337 22.1066V9.37598C22.1377 9.2708 22.0986 9.16873 22.0257 9.09313C21.9527 9.01736 21.8522 8.97463 21.7471 8.97463C21.6419 8.97463 21.5414 9.01736 21.4684 9.09313C21.3955 9.16873 21.3564 9.2708 21.3604 9.37598V22.1066C21.3604 22.4036 21.2424 22.6885 21.0324 22.8986C20.8222 23.1086 20.5375 23.2265 20.2404 23.2265H1.89343C1.5964 23.2265 