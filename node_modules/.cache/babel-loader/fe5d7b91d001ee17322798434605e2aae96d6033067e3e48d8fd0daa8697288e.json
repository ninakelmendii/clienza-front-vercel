{"ast":null,"code":"import CustomersTable from \"./CustomersTable.vue\";\nimport NavbarControl from \"../Navbar/NavbarControl.vue\";\nimport UserControl from \"../Navbar/UserControl.vue\";\nimport * as LANGUAGE from '../../store/language.json';\nexport default {\n  name: \"CustomersComponent\",\n  data() {\n    return {\n      title: \"Customers\",\n      search: \"\"\n    };\n  },\n  methods: {\n    translate(word) {\n      let wordIndex = null;\n      LANGUAGE.forEach((lng, index) => {\n        if (lng.english.toLowerCase() == word.toLowerCase()) {\n          wordIndex = index;\n        }\n      });\n      if (wordIndex === null) {\n        return word;\n      }\n      if (this.$store.state.language == 'german') {\n        return LANGUAGE[wordIndex].german;\n      } else {\n        return LANGUAGE[wordIndex].english;\n      }\n    }\n  },\n  components: {\n    CustomersTable,\n    NavbarControl,\n    UserControl\n  }\n};","map":{"version":3,"names":["CustomersTable","NavbarControl","UserControl","LANGUAGE","name","data","title","search","methods","translate","word","wordIndex","forEach","lng","index","english","toLowerCase","$store","state","language","german","components"],"sources":["src/components/Customers/Customers.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <v-row>\r\n      <v-col cols=\"12\" md=\"12\" lg=\"12\">\r\n        <v-row>\r\n          <v-row style=\"margin-left: 12px;\">\r\n            <v-col cols=\"12\" md=\"8\" class=\"d-flex justify-start\">\r\n              <NavbarControl />\r\n              <h2 style=\"display: flex; align-items: center;\">{{ translate(title) }}</h2>\r\n            </v-col>\r\n            <v-col cols=\"12\" md=\"4\"><UserControl /></v-col>\r\n          </v-row>\r\n\r\n          <v-col cols=\"12\" md=\"12\" lg=\"12\" style=\"margin-top: 24px;\">\r\n            <CustomersTable />\r\n          </v-col>\r\n        </v-row>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimp