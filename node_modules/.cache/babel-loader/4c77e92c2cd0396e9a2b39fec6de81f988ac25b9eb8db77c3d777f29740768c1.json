{"ast":null,"code":"import * as LANGUAGE from '../../store/language.json';\nexport default {\n  name: 'SeventhStep',\n  props: ['defaultData'],\n  data() {\n    return {\n      menu: false,\n      date: null,\n      info: {\n        real_estate: false,\n        stauern: false,\n        credit: false,\n        career: false,\n        bank: false,\n        remarks: null\n      }\n    };\n  },\n  methods: {\n    translate(word) {\n      let wordIndex = null;\n      LANGUAGE.forEach((lng, index) => {\n        if (lng.english.toLowerCase() == word.toLowerCase()) {\n          wordIndex = index;\n        }\n      });\n      if (wordIndex === null) {\n        return word;\n      }\n      if (this.$store.state.language == 'german') {\n        return LANGUAGE[wordIndex].german;\n      } else {\n        return LANGUAGE[wordIndex].english;\n      }\n    },\n    bindParams() {\n      if (this.defaultData != undefined) {\n        this.info = this.defaultData;\n      }\n    }\n  },\n  created() {\n    this.bindParams();\n    setTimeout(() => {\n      this.bindParams();\n    }, 1000);\n  }\n};","map":{"version":3,"names":["LANGUAGE","name","props","data","menu","date","info","real_estate","stauern","credit","career","bank","remarks","methods","translate","word","wordIndex","forEach","lng","index","english","toLowerCase","$store","state","language","german","bindParams","defaultData","undefined","created","setTimeout"],"sources":["src/components/Steps/SeventhStep.vue"],"sourcesContent":["<template>\r\n    <div style=\"display: flex; justify-content: space-between;\">\r\n        <div style=\"display: flex\">\r\n            <img src=\"@/assets/images/protocol/SeventhStep.png\" class=\"step-img \" />\r\n        </div>\r\n        <div style=\"width: 100%; text-align: center; margin-left: 25px;\">\r\n            <span style=\"font-weight: 600; font-size: 24px; line-height: 30px; font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Ope